{
    "prettier.printWidth": 160,
    "prettier.tabWidth": 4,
    "prettier.semi": false,
    "prettier.quoteProps": "consistent",
    "prettier.singleQuote": false,
    
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "deno.enable": true,
    "deno.lint": false,
    "deno.unstable": false
}
